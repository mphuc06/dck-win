{
  "name": "Oprekin 11 24H2(250)",
  "service": "windows",
  "containerEnv": {
    "VERSION": "https://archive.org/download/win-11-pro-24-h-2.3775-oprekin.com/Win11Pro_24H2.3775_oprekin.com.iso"
  },
  "forwardPorts": [8006],
  "portsAttributes": {
    "8006": {
      "label": "Web",
      "onAutoForward": "notify"
    }
  },
  "otherPortsAttributes": {
    "onAutoForward": "ignore"
  }, 
  "dockerComposeFile": "../codespaces.yml",
  "initializeCommand": "docker system prune --all --force"
}
